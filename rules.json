  "default_category": "Uncategorised",
  "rules": [
    {
      "name": "Groceries - PaknSave",
      "match": {
        "contains_any": [
          "pak n save",
          "paknsave",
          "pns"
        ]
      },
      "category": "Groceries"
    },
    {
      "name": "Utilities - Power",
      "match": {
        "regex_any": [
          "\\bmeridian\\b",
          "\\bcontact energy\\b",
          "\\bgenesis energy\\b"
        ]
      },
      "category": "Utilities"
    },
    {
      "name": "Transport - Fuel",
      "match": {
        "contains_any": [
          "bp ",
          "z energy",
          "caltex",
          "waitomo"
        ]
      },
      "category": "Transport"
    },
    {
      "name": "Dining/Takeaway",
      "match": {
        "contains_any": [
          "mcdonald",
          "burger",
          "pizza",
          "cafe",
          "coffee",
          "restaurant"
        ]
      },
      "category": "Dining"
    },
    {
